#include<iostream>
#include<algorithm>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n,m;
    cin>>n>>m;
    string s;
    cin>>s;
    sort(s.begin(),s.end());
    for(int i=1; i<=m; i++)
    {
        int k;
        cin>>k;
        cout<<s[k-1];
    }
    cout<<"\n";

}
